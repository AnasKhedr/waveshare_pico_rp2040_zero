{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "build": true
    },
    "explorerExclude.backup": null,
    "files.associations": {
        "*.ipp": "cpp",
        "istream": "cpp",
        "*.def": "cpp",
        "*.tcc": "cpp",
        "forward_list": "cpp",
        "list": "cpp",
        "slist": "cpp",
        "*.cpp": "cpp",
        "*.hpp": "cpp",
        "*.h": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "ranges": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "cinttypes": "cpp",
        "typeinfo": "cpp",
        "map": "cpp",
        "*.conf": "bitbake",
        "*.inc": "bitbake"
    },
    "C_Cpp.intelliSenseEngine": "disabled",
    "clangd.detectExtensionConflicts": false,
    "clangd.inactiveRegions.opacity": 0.55,
    "clangd.arguments": [
        "--compile-commands-dir=${workspaceFolder}\\.pio\\build",
        "--completion-style=detailed",
    ],
    "editor.formatOnSave": true,
    "cSpell.words": [
        "Pico",
    ],
    "C/C++ Include Guard.Macro Type": "Filename",
    "C/C++ Include Guard.Auto Include Guard Insertion For New File": true,
    "C/C++ Include Guard.Prefix": "_",
    "C/C++ Include Guard.Suffix": "_HPP",
    "C/C++ Include Guard.Comment Style": "Line",
}